{
    "name": "vladdnepr/yii2-ycm",
    "description": "YCM - Yii 2 Content Management module",
    "keywords": ["yii2", "yii2-ycm", "admin", "administration", "module", "extension"],
    "homepage": "https://github.com/vladdnepr/yii2-ycm",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
          "name": "Vladislav Lyshenko",
          "email": "vladdnepr1989@gmail.com"
        },
        {
            "name": "Jani Mikkonen",
            "email": "janisto@php.net"
        }
    ],
    "support": {
        "issues": "https://github.com/vladdnepr/yii2-ycm/issues?state=open",
        "source": "https://github.com/vladdnepr/yii2-ycm"
    },
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "janisto/yii2-timepicker": "*",
        "vova07/yii2-imperavi-widget": "*",
        "kartik-v/yii2-grid": "~3.0",
        "kartik-v/yii2-krajee-base": "~1.7",
        "kartik-v/yii2-mpdf": "~1.0",
        "kartik-v/yii2-editable": "^1.7",
        "kartik-v/yii2-widgets": "^3.4",
        "kartik-v/yii2-field-range": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "vladdnepr\\ycm\\": "src"
        }
    },
    "extra": {
        "bootstrap": "vladdnepr\\ycm\\Bootstrap",
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
